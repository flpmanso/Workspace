
public class Conta {
	int numero;
	String dono;
	double saldo;
	double limite;
	double salario;
	
	boolean saca(double quantidade) {
		if(this.saldo < quantidade){
			return false;
			
		}
		else {
			this.saldo -= quantidade;
			return true;
		}
			
		
	}
		
	
	void deposita(double quantidade) {
		this.saldo += quantidade;
	}
}
