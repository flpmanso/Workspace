package entities;

import java.util.Date;

public class Aluno {
	
	private int matricula;
	private String nome;
	private Date data;
	public Aluno(int matricula, String nome, Date data) {
		this.matricula = matricula;
		this.nome = nome;
		this.data = data;
	}
	public int getMatricula() {
		return matricula;
	}
	public void setMatricula(int matricula) {
		this.matricula = matricula;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public Date getData() {
		return data;
	}
	public void setData(Date data) {
		this.data = data;
	}
	
	

}
